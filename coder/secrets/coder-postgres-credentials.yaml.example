# Example: coder-postgres-credentials
# Copy to coder-postgres-credentials.yaml, replace password, then apply
# DO NOT commit the actual secret to git
#
# kubectl apply -f coder-postgres-credentials.yaml
#
apiVersion: v1
kind: Secret
metadata:
  name: coder-postgres-credentials
  namespace: coder
type: Opaque
stringData:
  username: coder
  password: "REPLACE_WITH_SECURE_PASSWORD"
